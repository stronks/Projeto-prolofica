<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title - Prologica</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body class="container body-content">
  <nav class="navbar navbar-expand-lg navbar-light bg-light">
    @Html.ActionLink("Prologica ", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>

    <div class="collapse navbar-collapse" id="navbarSupportedContent">
      <ul class="navbar-nav mr-auto">
        <li class="nav-item active">
        <li>@Html.ActionLink("Agendamentos", "Agendamentos", "Home", new { @class = "nav-link" })</li>
        </li>
        <li class="nav-item">
          @Html.ActionLink("Medicos", "Medicos", "Home", null, new { @class = "nav-link" })

        </li>
        <li class="nav-item">
          @Html.ActionLink("Pacientes", "Pacientes", "Home", null, new { @class = "nav-link" })

        </li>
        <li class="nav-item">
          @Html.ActionLink("Consultorios", "Consultorios", "Home", null, new { @class = "nav-link" })

        </li>

      </ul>

    </div>
  </nav>
  <div class="container body-content" style="margin-top: 54px;">
    @RenderBody()
    @RenderSection("SPAViews", required: false)
    <hr />
    <footer>
      <p>&copy; @DateTime.Now.Year - Prologia - Feito por Wagner Soares</p>
    </footer>
  </div>

  @Scripts.Render("~/bundles/jquery")
  @Scripts.Render("~/bundles/knockout")


  @Scripts.Render("~/bundles/bootstrap")
  @RenderSection("Scripts", required: false)
</body>
</html>
